{
  "private": true,
  "name": "@static-pages/starter",
  "version": "0.2.6",
  "description": "Wires together a basic set of packages.",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.mts",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.mjs",
      "types": "./index.d.mts"
    },
    "./node": {
      "require": "./node.cjs",
      "import": "./node.mjs",
      "types": "./node.d.mts"
    },
    "./node.cjs": {
      "require": "./node.cjs",
      "types": "./node.d.cts"
    },
    "./node.mjs": {
      "import": "./node.mjs",
      "types": "./node.d.mts"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=16.0.0",
    "deno": ">=1.0.0"
  },
  "scripts": {
    "clean": "node scripts/clean.mjs",
    "build": "node scripts/build.mjs",
    "watch": "node scripts/watch.mjs",
    "test": "mocha",
    "coverage": "c8 -r text -r text-summary -r lcov --include \"**/*.mjs\" npm test",
    "prepublishOnly": "echo Default 'npm publish' disabled, use 'npm run publish' instead && exit 1",
    "publish": "npm run build && npm run test && cd package && npm publish",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@static-pages/core": "^7.0.0",
    "@static-pages/fs": "^1.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@types/node": "^22.13.5",
    "babel-plugin-replace-import-extension": "^1.1.4",
    "c8": "^10.1.3",
    "chokidar": "^4.0.3",
    "mocha": "^11.1.0",
    "semver": "^7.6.2",
    "typescript": "^5.4.5"
  },
  "author": "László BULIK",
  "license": "MPL-2.0",
  "homepage": "https://staticpagesjs.github.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staticpagesjs/starter.git"
  },
  "bugs": {
    "url": "https://github.com/staticpagesjs/starter/issues"
  },
  "keywords": [
    "static",
    "pages",
    "generator",
    "renderer",
    "mvc",
    "jamstack",
    "view",
    "template",
    "controller"
  ]
}
