{
  "name": "@static-pages/starter",
  "version": "0.0.4",
  "description": "Wires together a basic set of packages.",
  "type": "module",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "types": "./types/index.d.js"
    },
    "./package.json": "./package.json",
    "./node": {
      "import": "./esm/node.js",
      "require": "./cjs/node.js",
      "types": "./types/node.d.js"
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "deno": ">=1.0.0"
  },
  "scripts": {
    "preversion": "npm run build && npm run test",
    "postversion": "git push && git push --tags",
    "clean": "rimraf esm cjs types coverage",
    "prewatch:esm": "npm run build:esm",
    "watch:esm": "tsc --watch",
    "prewatch:cjs": "npm run build:cjs",
    "watch:cjs": "tsc --outDir cjs --module commonjs --moduleResolution node --watch",
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc",
    "build:cjs": "tsc --outDir cjs --module commonjs --moduleResolution node",
    "postbuild:cjs": "echo { \"type\": \"commonjs\" }>cjs/package.json",
    "build:types": "tsc --outDir types --declaration --emitDeclarationOnly",
    "lint": "eslint",
    "test": "mocha",
    "coverage": "c8 -r text -r text-summary -r lcov --include \"esm/*\" npm test"
  },
  "dependencies": {
    "@static-pages/core": "^7.0.0-alpha.6",
    "@static-pages/io": "^0.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "c8": "^8.0.0",
    "eslint": "^8.1.0",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  },
  "author": "László BULIK",
  "license": "MPL-2.0",
  "homepage": "https://staticpagesjs.github.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staticpagesjs/starter.git"
  },
  "bugs": {
    "url": "https://github.com/staticpagesjs/starter/issues"
  },
  "keywords": [
    "static",
    "pages",
    "generator",
    "renderer",
    "mvc",
    "jamstack",
    "view",
    "template",
    "controller"
  ]
}
